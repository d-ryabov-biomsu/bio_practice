    "def get_species(record, df):\n",
    "    acc = record.id\n",
    "    request = df.loc[df['accession'] == acc]['organism']\n",
    "    try:\n",
    "        species = request.iloc[0]\n",
    "    except IndexError:\n",
    "        print('species error', record.description, record.seq, '\\n')\n",
    "        return None\n",
    "    else:\n",
    "        return species\n"
